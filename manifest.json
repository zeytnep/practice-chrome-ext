{
	"manifest_version": 3,
    "name": "screen shot taker",
    "description": "Extension to take screen shots of web pages",
    "permissions": ["storage",  "tabs"],
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "js":["contentScript.js"]
      }
    ]
}